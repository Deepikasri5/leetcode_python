def isAnagram(s, t):
    dic ={}
    for c in s:
        dic[c] = dic.get(c, 0)+1
    for d in t:
        if d not in dic:
            return False
        dic[d] -=1
        if dic[d] < 0:
            return False
        
    return True


def main():
    result = isAnagram("anagram", "nagarama")
    print("result", result)

main()
        